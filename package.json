{
  "name": "wikiprofile",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:frontend": "nuxt dev ./frontend",
    "dev:directus": "env CONFIG_PATH=./directus/config.js directus start",
    "start": "npm run start:directus && npm run start:frontend",
    "start:frontend": "nuxt build ./frontend && pm2 startOrReload ./deployment/frontend.config.js",
    "start:directus": "pm2 startOrReload ./deployment/directus.config.js",
    "stop": "npm run stop:directus && npm run stop:frontend",
    "stop:frontend": "pm2 delete ./deployment/frontend.config.js",
    "stop:directus": "pm2 delete ./deployment/directus.config.js",
    "deploy": "npm run deploy:directus && npm run deploy:frontend",
    "deploy:frontend": "pm2 deploy ./deployment/frontend.config.js production update",
    "deploy:setup:frontend": "pm2 deploy ./deployment/frontend.config.js production setup",
    "deploy:directus": "pm2 deploy ./deployment/directus.config.js production update",
    "deploy:setup:directus": "pm2 deploy ./deployment/directus.config.js production setup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikisoft-code/io.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/wikisoft-code/io/issues"
  },
  "homepage": "https://github.com/wikisoft-code/io#readme",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.15.3",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.3",
    "@fortawesome/pro-solid-svg-icons": "^5.15.3",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@pnodev/nuxt-directus": "^0.2.0",
    "animate.css": "^4.1.1",
    "cookie": "^0.4.1",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.9.1",
    "directus": "^9.0.0-rc.82",
    "dotenv": "^10.0.0",
    "figlet": "^1.5.0",
    "inquirer": "^8.1.1",
    "js-cookie": "^2.2.1",
    "minimist": "^1.2.5",
    "nuxt": "^2.4.5",
    "nuxt-buefy": "^0.4.7",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "pg-minify": "^1.6.2",
    "pm2": "^5.1.0",
    "remove": "^0.1.5",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.1",
    "vue-recaptcha": "^1.3.0",
    "vue2-google-maps": "^0.10.7",
    "vuex-persistedstate": "^4.0.0-beta.3"
  }
}
